# ~/.bash_aliases

alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

alias k='kubectl'

alias runopenui='run-openui --replace --skip-openui-build'

alias explorer='/mnt/c/Windows/explorer.exe .'

alias update='sudo -- sh -c "apt update && apt upgrade"'

alias gitdm='git diff master --name-only'
alias gitmm='git merge master'
alias gitsu='git submodule update'

alias up='cd ..'
alias up2='cd ../..'
alias up3='cd ../../..'
